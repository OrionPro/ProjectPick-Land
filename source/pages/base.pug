								.col-xl-4.col-12
									.footer__sign-up
										p Подпишись и получи скидку
										.footer__sign-up-widget
						.row
							.col-12
								span.copyright Copyright Pickcase 2017

		div(off-canvas='id-1 left shift')
			nav.header_nav_mobile
				ul.menu
					-var links = [{title:'Отзывы', attr: 'join', href: '#', classNameLink: 'scroll_to'},{title:'Доставка и оплата', dataEffect: 'mfp-zoom-in', classNameLink: 'open-popup-link', href: '#popup-delivery'},{title:'Свой дизайн', href: '#', attr: 'ready-to-create__constructor', classNameLink: 'scroll_to'},{title:'+38 (096) 098-20-12', href:'tel:+380960982012'},{title:'+38 (066) 851-87-43', href:'tel:+380668518743'},{title:'pickcase', href:'skype:pickcase?call'},{title:'smile@pickcase.com.ua', href:'mailto:smile@pickcase.com.ua'},{title:'Не дозвонились? Заказ звонка!', dataEffect: 'mfp-zoom-in', classNameLink: 'open-popup-link', href:'#popup'}];
					each i in links
						li(class=i.className)
							a(href=i.href data-scroll=i.attr data-effect=i.dataEffect class=i.classNameLink)= i.title
